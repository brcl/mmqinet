<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <AssemblyName>Mmqi.Net</AssemblyName>
    <RootNamespace>Mmqi</RootNamespace>
    <Version>0.2.0</Version>
    <Authors>Facundo Glaeser</Authors>
    <Company />
    <RepositoryUrl>https://github.com/fglaeser/mmqinet.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Minimalist .Net Core interface for IBM MQ (WebSphere MQ, MQSeries)</Description>
    <PackageTags>mqi;ibm;mq</PackageTags>
    <PackageReleaseNotes>Add subscriptions.</PackageReleaseNotes>
    <AssemblyVersion>0.2.0.0</AssemblyVersion>
    <FileVersion>0.2.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NETCOREAPP;NETCOREAPP2_1;</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;RELEASE;NETCOREAPP;NETCOREAPP2_1;</DefineConstants>
  </PropertyGroup>

</Project>
